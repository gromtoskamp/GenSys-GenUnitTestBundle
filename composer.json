{
    "name": "gensys/generatebundle",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.0.8",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "nette/php-generator": "^3.2",
        "symfony/console": "3.4.*",
        "symfony/dotenv": "3.4.*",
        "symfony/symfony": "3.4.*",
        "symfony/framework-bundle": "3.4.*",
        "symfony/maker-bundle": "^1.14",
        "symfony/yaml": "3.4.*"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.1",
        "phpunit/phpunit": "5.5.* || ^6.5.14 || ^7.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "GenSys\\GenerateBundle\\": "src/GenSys/GenerateBundle/",
            "App\\Tests\\Unit\\": "tests/Unit",
            "Tests\\Unit\\": "tests/Unit"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "3.4.*"
        }
    }
}
